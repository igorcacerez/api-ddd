{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:vitest-globals/recommended"
  ],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "indent": ["error", "tab"],
    "quotes": ["error", "single"]
  },
  "env": {
    "vitest-globals/env": true
  }
}
